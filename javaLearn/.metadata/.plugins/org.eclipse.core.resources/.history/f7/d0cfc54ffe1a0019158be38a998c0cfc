
public class Student extends Person {

	public Student(String name) {
		super(name);
		// TODO Auto-generated constructor stub
	}
	private int score;


	public int getScore() {
		return score;
	}

	public void setScore(int score) {
		this.score = score;
	}
	public String hello(){
		return "Hello, " + this.name;
	}
	
	public void run(){
		System.out.println("student" + name + " is running.");
	}

}
